{
	"name": "palette-shift",
	"description": "Modify beautiful hand-picked palettes to suit your exact needs",
	"keywords": [
		"palette-shift",
		"palette"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/babichjacob/sapper-firebase-typescript-graphql-tailwindcss-actions-template.git"
	},
	"license": "MIT",
	"version": "2020.09.04",
	"scripts": {
		"eslint": "eslint",
		"eslint:fix": "eslint --fix ./*.js ./src/*.ts ./src/components/**/*.svelte ./src/routes/**/*.svelte",
		"validate": "svelte-check --ignore src/node_modules/@sapper,.history",
		"validate:dev": "svelte-check --ignore src/node_modules/@sapper,.history --watch",
		"firebase": "firebase",
		"firebase:deploy": "firebase deploy",
		"firebase:deploy:hosting": "firebase deploy --only hosting",
		"firebase:deploy:functions": "firebase deploy --only functions",
		"sapper:dev": "sapper dev --open",
		"sapper:build": "cross-env NODE_ENV=production sapper build --legacy",
		"sapper:export": "cross-env NODE_ENV=production sapper export --legacy",
		"static:rmdir": "rimraf - __sapper__/export",
		"static:mkdir": "mkdirp __sapper__/export",
		"static:cp": "ncp static __sapper__/export",
		"static:404:cp": "ncp __sapper__/export/404/index.html __sapper__/export/404.html",
		"static:404:rmdir": "rimraf - __sapper__/export/404",
		"dev": "run-p sapper:dev",
		"prod": "run-s sapper:build static:rmdir static:mkdir static:cp",
		"start": "node __sapper__/build",
		"deploy": "run-s prod firebase:deploy",
		"export": "run-s sapper:export static:404:cp static:404:rmdir",
		"deploy:export": "run-s export firebase:deploy:hosting"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"express": "^4.17.1",
		"firebase-admin": "^9.1.1",
		"firebase-functions": "^3.11.0",
		"firebase-tools": "^8.10.0",
		"node-fetch": "^2.6.0",
		"sirv": "^1.0.6"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/runtime": "^7.11.2",
		"@fullhuman/postcss-purgecss": "^2.3.0",
		"@iconify-icons/mdi": "^1.0.3",
		"@iconify/svelte": "^1.0.2",
		"@rollup/plugin-babel": "^5.2.0",
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-typescript": "^5.0.2",
		"@tailwindcss/custom-forms": "^0.2.1",
		"@tailwindcss/ui": "^0.5.0",
		"@types/chroma-js": "^2.0.0",
		"@types/color-namer": "^1.3.0",
		"@types/compression": "^1.7.0",
		"@types/express": "^4.17.8",
		"@types/node-fetch": "^2.5.7",
		"@typescript-eslint/eslint-plugin": "^4.0.1",
		"@typescript-eslint/parser": "^4.0.1",
		"apollo-server-express": "^2.17.0",
		"bufferutil": "^4.0.1",
		"class-validator": "^0.12.2",
		"color-namer": "^1.4.0",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"d3-color": "^2.0.0",
		"d3-color-difference": "^0.1.3",
		"eslint": "^7.8.1",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-svelte3": "^2.7.3",
		"google-fonts-complete": "github:crswll/google-fonts-complete#rerun-font-fetch",
		"graphql": "^15.3.0",
		"lodash-es": "^4.17.15",
		"mkdirp": "^1.0.4",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^7.0.32",
		"postcss-font-magician": "^2.3.1",
		"postcss-import": "^12.0.1",
		"postcss-load-config": "^2.1.0",
		"postcss-nested": "^4.2.3",
		"postcss-preset-env": "^6.7.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rollup": "^2.26.10",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-visualizer": "^4.1.1",
		"sapper": "^0.28.4",
		"svelte": "^3.24.1",
		"svelte-check": "^1.0.35",
		"svelte-preprocess": "^4.2.1",
		"tailwindcss": "^1.8.0",
		"tslib": "^2.0.1",
		"type-graphql": "^1.0.0",
		"typescript": "^4.0.2",
		"utf-8-validate": "^5.0.2"
	},
	"engines": {
		"node": "10"
	}
}
